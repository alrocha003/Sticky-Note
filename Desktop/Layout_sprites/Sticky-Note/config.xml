<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>Sticky Note</name>
  
    <platform name="android">
              <icon src="icon.png" density="ldpi" />
              <icon src="icon.png" density="mdpi" />
              <icon src="icon.png" density="hdpi" />
              <icon src="icon.png" density="xhdpi" />
     </platform>
    
  <description>
      Sticky Note - Notas rapidas para o seu dia a dia.
  </description>

    <platform name="android">
    <!-- you can use any density that exists in the Android project -->
    <splash src="splash_screen_fw.png" density="land-hdpi"/>
    <splash src="splash_screen_fw.png" density="land-ldpi"/>
    <splash src="splash_screen_fw.png" density="land-mdpi"/>
    <splash src="splash_screen_fw.png" density="land-xhdpi"/>

    <splash src="splash_screen_fw.png" density="port-hdpi"/>
    <splash src="splash_screen_fw.png" density="port-ldpi"/>
    <splash src="splash_screen_fw.png" density="port-mdpi"/>
    <splash src="splash_screen_fw.png" density="port-xhdpi"/>
</platform>

</widget>
